<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/time_you2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="3dp"
        android:text="몇시 몇분"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/home_04_chat_main"
        app:layout_constraintStart_toEndOf="@+id/home_04_chat_main" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/home_04_chat_profile2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:src="@mipmap/ic_launcher_round"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.7" />

    <TextView
        android:id="@+id/home_04_chat_nick2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/home_04_chat_main"
        app:layout_constraintTop_toTopOf="@+id/home_04_chat_profile" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.3" />

    <TextView
        android:id="@+id/time_me2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="3dp"
        android:text="TextView"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/home_04_chat_main_me"
        app:layout_constraintEnd_toStartOf="@+id/home_04_chat_main_me" />

    <TextView
        android:id="@+id/home_04_chat_read_you2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:text="0"
        android:textColor="@color/mybrary_main"
        android:textSize="8sp"
        app:layout_constraintBottom_toTopOf="@+id/time_you"
        app:layout_constraintStart_toStartOf="@+id/time_you" />

    <TextView
        android:id="@+id/home_04_chat_read_me2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:text="0"
        android:textColor="@color/mybrary_main"
        android:textSize="8sp"
        app:layout_constraintBottom_toTopOf="@+id/time_me"
        app:layout_constraintEnd_toEndOf="@+id/time_me" />
</android.support.constraint.ConstraintLayout>